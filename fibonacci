#include <stdio.h>

int main()
{
    int a, b, fib;
    int num, i;
    
    a=0;
    b=1;
    
    printf("\nEntre com um numero:");
    scanf("%d", &num);
    
    printf("0, ");
    
    if(num>=2);
    printf("1, ");
    
    for(i=3;i<=num;i++)
    {
                       fib=a+b;
                       a=b;
                       b=fib;
                       
                       printf("%d, ",fib);
    }
    
printf("\n\n");

system("pause");
return 0;

}
