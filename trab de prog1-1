#include<stdio.h>

int main()
{
	int i, num;
	
	printf("Entre com um mes:");
	scanf("%d", &num);
	
	if(num==2)
	{
		for(i=1;i<29;i++)
		{
			printf("%d/%d/2016\n", i,num);
		}
	}


else if((num==4)||(num==6)||(num==9)||(num==11))
{
	for(i=1;i<31;i++)
	{
		printf("%d/%d/2016\n", i,num);
	}
}

else 
{
	for(i=1;i<32;i++)
	{
		printf("%d/%d/2016\n",i, num);
	}
}

system("pause");

return 0;

}
